{
    "name": "dragonmantank/mini-di",
    "type": "library",
    "description": "Very basic Dependency Injection system.",
    "keywords": ["dependency injection", "inversion of control"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Tankersley",
            "email": "chris@ctankersley.com",
            "homepage": "https://github.com/dragonmantank"
        }
    ],
    "require": {
        "php": "^7.3",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11",
        "phpunit/phpunit": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "MiniDI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiniDI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "./vendor/bin/phpstan analyse -l 6 src"
    }
}
